	Journal = {Physics Letters B},
	Number = {1},
	Pages = {46 - 54},
	Title = {The \{QCD\} transition temperature: Results with physical masses in the continuum limit},
	Url = {http://www.sciencedirect.com/science/article/pii/S0370269306012755},
	Volume = {643},
	Year = {2006},
