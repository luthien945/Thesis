	Journal = {Nuclear Physics A},
	Number = {3--4},
	Pages = {413 - 446},
	Title = {Dilepton production from \{AGS\} to \{SPS\} energies within a relativistic transport approach},
	Url = {http://www.sciencedirect.com/science/article/pii/S0375947497001401},
